<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.memberapps2">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <application
        android:name="com.yisc.AppController"
        android:allowBackup="true"
        android:icon="@drawable/ic_logo3"
        android:label="YISC Al Azhar"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name="entity.BroadcastService" />
        <activity
            android:name="com.yisc.Home"
            android:label="YISC Al Azhar"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.yisc.LoginActivity"
            android:label="YISC Al Azhar"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true"/>
        <activity android:name="com.yisc.WelcomeActivity"
            android:label="YISC Al Azhar"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true"/>
        <activity android:name="com.yisc.VideoKajianActivity"
            android:label="YISC Al Azhar"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.yisc.ResumeActivity"
            android:label="YISC Al Azhar"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.yisc.QuestionActivity1"
            android:label="YISC Al Azhar"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true"/>
    </application>

</manifest>